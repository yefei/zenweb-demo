{
  "name": "webapp",
  "private": true,
  "main": "app/index.js",
  "scripts": {
    "start": "node --enable-source-maps app",
    "build": "rimraf app && tsc",
    "dbgen": "zenorm-generate .dbgen.json",
    "bench": "npm run build && node --inspect app",
    "dev-start": "node -r dotenv/config --enable-source-maps app",
    "dev": "rimraf app && tsc-watch --onSuccess \"npm run dev-start\""
  },
  "dependencies": {
    "@zenweb/cors": "^2.4.0",
    "@zenweb/form": "^3.7.2",
    "@zenweb/grid": "^3.1.2",
    "@zenweb/mysql": "^3.5.2",
    "@zenweb/upload": "^3.1.0",
    "@zenweb/view": "^2.4.1",
    "debug": "^4.3.2",
    "moment": "^2.29.1",
    "zenorm": "^3.27.2",
    "zenweb": "^3.15.1"
  },
  "devDependencies": {
    "@zenorm/generate": "^1.3.1",
    "@zenorm/generate-mysql": "^1.2.0",
    "dotenv": "^16.0.3",
    "rimraf": "^4.4.0",
    "tsc-watch": "^6.0.0",
    "typescript": "^5.0.2"
  }
}
