{
  "name": "webapp",
  "private": true,
  "main": "app/index.js",
  "scripts": {
    "start": "node app",
    "clean": "tsc --build --clean",
    "build": "yarn clean && tsc",
    "dbgen": "zenorm gen .dbconfig.json",
    "dev-node": "cross-env NODE_ENV=development APP_ENV=dev DEBUG=* node --enable-source-maps app",
    "dev": "zenweb-service-types && tsc-watch --onSuccess 'yarn dev-node'"
  },
  "dependencies": {
    "@zenweb/cors": "^2.3.0",
    "@zenweb/form": "^2.6.0",
    "@zenweb/grid": "^2.11.1",
    "@zenweb/mysql": "^2.4.0",
    "@zenweb/orm": "^2.4.1",
    "debug": "^4.3.2",
    "moment": "^2.29.1",
    "zenorm": "^3.0.5",
    "zenweb": "^2.4.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.4"
  }
}
