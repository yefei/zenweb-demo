{
  "name": "webapp",
  "private": true,
  "main": "app/index.js",
  "scripts": {
    "start": "node --enable-source-maps app",
    "build": "rimraf app && tsc",
    "dbgen": "zenorm gen .dbgen.json",
    "dev-node": "cross-env NODE_ENV=development DEBUG=*,-zenweb:core:module:metric,-koa-router node --enable-source-maps app",
    "dev": "rimraf app && tsc-watch --onSuccess \"yarn dev-node\""
  },
  "dependencies": {
    "@zenweb/cors": "^2.3.0",
    "@zenweb/form": "^3.5.0",
    "@zenweb/grid": "^3.0.4",
    "@zenweb/mysql": "^2.5.0",
    "@zenweb/orm": "^2.4.1",
    "@zenweb/view": "^2.4.1",
    "debug": "^4.3.2",
    "moment": "^2.29.1",
    "zenorm": "^3.18.1",
    "zenweb": "^3.0.22"
  },
  "devDependencies": {
    "@zenorm/mysql": "^1.0.2",
    "cross-env": "^7.0.3",
    "rimraf": "^3.0.2",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.4"
  }
}
